{
  "name": "mohanhome",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite dev",
    "build": "bunx --bun vite build ",
    "start": "bun run .output/server/index.mjs",
    "check": "bunx ultracite check",
    "fix": "bunx ultracite fix --unsafe",
    "db": "drizzle-kit",
    "seed": "bun src/lib/drizzle/seed.ts",
    "auth": "bun x --bun @better-auth/cli",
    "auth:secret": "bun x --bun @better-auth/cli secret",
    "auth:generate": "bun x --bun @better-auth/cli generate --config ./src/lib/auth/auth.ts --y --output ./src/lib/db/schema/auth.schema.ts && prettier --write ./src/lib/db/schema/auth.schema.ts",
    "auth:migrate": "bun x --bun @better-auth/cli migrate --config ./src/lib/auth/auth.ts",
    "test": "bun test",
    "coverage": "bun test --coverage",
    "ui": "bun x shadcn@canary",
    "prepare": "husky"
  },
